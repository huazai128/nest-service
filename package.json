{
  "name": "nest-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "NODE_ENV=development concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "NODE_ENV=development nodemon --config nodemon-debug.json",
    "prestart:prod": "NODE_ENV=production rimraf dist && npm run build",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/typeorm": "^6.1.2",
    "@typegoose/typegoose": "^6.0.4",
    "body-parser": "^1.19.0",
    "cache-manager": "^2.10.0",
    "cache-manager-redis-store": "^1.5.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "cluster": "^0.7.7",
    "compression": "^1.7.4",
    "express-rate-limit": "^5.0.0",
    "fabric": "^4.6.0",
    "fs-extra": "^9.0.1",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.7.6",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-auto-increment-reworked": "^1.2.1",
    "mongoose-paginate": "^5.0.3",
    "nestjs-config": "^1.4.7",
    "nestjs-typegoose": "^7.0.0",
    "node-canvas": "^2.7.0",
    "node-schedule": "^1.3.2",
    "phantom": "^6.3.0",
    "phantomjs": "^2.1.7",
    "qiniu": "^7.2.2",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "typegoose": "^5.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/fabric": "^4.5.4",
    "@types/jest": "^23.3.13",
    "@types/mongoose": "^5.5.23",
    "@types/mongoose-auto-increment": "^5.0.30",
    "@types/mongoose-paginate": "^5.0.6",
    "@types/node": "^10.12.18",
    "@types/phantom": "^3.2.5",
    "@types/redis": "^2.8.14",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "jest": "^29.3.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.10.1",
    "tslint": "5.16.0",
    "typescript": "^3.7.5",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
